<h4><%= category_name.capitalize %></h4>
<table>
	<thead>
		<tr>
			<!-- <th> Checkbox</th> -->
			<th><!-- Icon --></th>
			<th>Branch</th>
			<th>Display</th>
			<th>Version</th>
			<th>Actions</th>
			<th>
				Update
				<%= helpful_info("If a checkmark appears, there is a newer version of this package shared by another unit. Go to 'shared packages' to import.") %>
			</th>
		</tr>
	</thead>
	<% packages.each do |package| %>
		<%= render :partial => 'package_table_row', :locals => {:package => package} %>
	<% end -%>
</table>