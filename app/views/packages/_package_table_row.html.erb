<tr>
	<!-- <td>
		<input type="checkbox" />
	</td> -->
	<td><%= image_tag(package.icon.url(:small)) %></td>
	<td><%= package %></td>
	<td>
		<% package.versions.each do |pkg| %>
			<%= pkg.display_name %><br />
		<% end -%>
	</td>
	<td>
		<% package.versions.each do |pkg| %>
			<%= link_to "#{pkg.version}", package_path(pkg) %><br />
		<% end -%>
	</td>
	<td>
		<% package.versions.each do |pkg| %>
			<%= link_to 'Edit', edit_package_path(pkg) %> |
			<% if super_user? %>
				<%= link_to('Destroy', pkg, :confirm => "Are you sure you want to destroy #{pkg.to_s(:version)}?", :method => :delete) %> |
			<% end -%>
			<%= link_to 'Download', download_package_path(pkg.installer_item_location) %>
			<br />
		<% end -%>
	</td>
	<td>
		<%= "&#10003;".html_safe if package.package_branch.new_version_shared?(package.unit) %>
	</td>
</tr>