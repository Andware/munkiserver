<tr>
	<td rowspan="<%= package.versions.count %>"><%= image_tag(package.icon.url(:small)) if package.icon %></td>
	<td rowspan="<%= package.versions.count %>">
		<%= package.display_name %><br />
		<em><%= package.name %></em>
	</td>
	
		<%= render :partial => 'package_version_column', :locals => {:pkg => package.versions.first} %>
	

	<td rowspan="<%= package.versions.count %>">
		<% unless package.has_dependencies? %>
			<% if has_shared?(package) %>
				<%= link_to "import", import_shared_package_path(current_unit,import_package(package)), {"data-confirm-message" => "Are you sure you want to import #{package.name} #{import_package(package).version} from #{import_package_unit_name(package)} ?"} %>
			<% elsif package.package_branch.new_version? %>
				<%= link_to "update", version_tracker_info_url(package), :target => "_blank" %>
			<% end %>
		<% end %>
	</td>
</tr>
<% package.versions.each_with_index do |pkg,i| %>
	<% unless i == 0 %>
		<tr>
			<%= render :partial => 'package_version_column', :locals => {:pkg => pkg} %>
		</tr>
	<% end %>
<% end %>